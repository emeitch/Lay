{
  "_type": "App",
  "_contents": [
    {
      "_type": "Window",
      "_name": "task_list",
      "todos": {
        "_type": "Jack<[Todo]>"
      },
      "todo_creation": {
        "_type": "Plug<string>"
      },
      "_contents": [
        {
          "_type": "InputText",
          "_wires": [
              {
                "_type": "Wire",
                "to": "todo_creation",
                "from": "onKey"
              }
          ]
        },
        {
          "_type": "Ul",
          "_contents": {
            "_type": "BoxMap",
            "_wires": [
              {
                "_type": "Wire",
                "to": "array",
                "from": "todos"
              },
              {
                "_type": "Wire",
                "to": "box",
                "value": {
                  "_type": "Box",
                  "todo": {
                    "_type": "Jack<Todo>"
                  },
                  "_contents": {
                    "_type": "Li",
                    "_contents": {
                      "_type": "Span",
                      "_wires": [
                        {
                          "_type": "Wire",
                          "to": "textContent",
                          "value": {
                            "_type": "ObjectProperty",
                            "_wires": [
                              {
                                "_type": "Wire",
                                "to": "object",
                                "from": "todo"
                              },
                              {
                                "_type": "Wire",
                                "to": "property_name",
                                "value": "title"
                              }
                            ]
                          }
                        }
                      ]
                    }
                  }
                }
              }
            ]
          }
        },
        {
          "_type": "Switch",
          "_contents": [
              {
                "_type": "SwitchButton",
                "title": "All"
              },
              {
                "_type": "SwitchButton",
                "title": "Active"
              },
              {
                "_type": "SwitchButton",
                "title": "Completed"
              }
          ]
        }
      ]
    }
  ]
}